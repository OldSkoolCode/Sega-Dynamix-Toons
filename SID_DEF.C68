/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
*                                                                            *
*                                  SID_WHOP                                  *
*                                                                            *
* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
#define SID_WHOP_PLANE       SID_PLANE
#define SID_WHOP_PLANE1      SID_PLANE1
#define SID_WHOP_BORDERS     SID_BORDERS

/* * * * *
*
*  SID_WHOP_SEQUENCE--The non flipped version has
*  the mallot handle towards the right of the screen.  Sid is approaching
*  from the right, stops, grabs the mallot, and clobbers the cat, who
*  is chasing him.  The frame order for that sequence is as follows...
*  1,1,1,1,2,2,2,2,2,2,3,3, at which point Sid will drop the hammer 
*  and enter the next sequence.  If Sid is approaching from the left 
*  with the cat chasing him and the mallot handle facing the left, Sid 
*  will grab the hammer and whop the cat that is behind him.  This will 
*  be the same sequence with all flipped shapes.  If Sid is appraoching 
*  the mallot from the right but the mallot handle is toward the left... 
*  Sid will stop, turn around to grab the mallot, then whop the oncoming 
*  cat.  This sequence will use flipped 1,1,1,1, then normal 2,2,2,2,
*  2,2,3,3.  If Sid is approaching from the left but the mallot handle 
*  is facing the right, sid will stop, turn around, grab the mallot 
*  and whop the oncoming cat.  The order for this sequence will be 1,
*  1,1,1, then flipped 2,2,2,2,2,2,3,3, at this point Sid will drop 
*  the mallot and continue At the beggining of the sequence Sid will 
*  have an offset from the mallot of {27,1} for the normal sequence, 
*  and an offset of {-16,1} for the flipped version.
*
* * * * */

static F_SSHAPE_C sid_whop1 = {SID_LINK, SID_WHOP_PLANE, {0,255,255,255},
   {{-1,0},{-1,0},{-1,0},{-1,0}}};
static F_SSHAPE_C sid_whop2 = {SID_LINK, SID_WHOP_PLANE, {1,255,255,255},
   {{-13,-7},{-1,0},{-1,0},{-1,0}}};
static F_SSHAPE_C sid_whop3 = {SID_LINK, SID_WHOP_PLANE, {2,255,255,255},
   {{13,-18},{-1,0},{-1,0},{-1,0}}};


static CHARACTER_SEQ sid_whop_character =
   {
      SID_WHOP_NUM_STATES,
      SID_WHOP_INDEX,
   };

